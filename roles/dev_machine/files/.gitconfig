[user]
	name = Warren Harper
	email = warrenjharper@gmail.com
[merge]
	tool = meld
[diff]
	tool = meld
[push]
	default = simple
[clean]
	requireForce = false
[alias]
        lg = log --graph --pretty=format:'%Cred%h%Creset - %C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit 
	diff = diff -w
	last-diff = diff HEAD~1 HEAD
[branch]
	autosetuprebase = always
[color]
	ui = true
